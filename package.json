    "dev-generate": "prisma generate --schema=./prisma/schemas/artemis/schema.prisma && prisma generate --schema=./prisma/schemas/daphnis/schema.prisma",
    "daphnis:drop": "prisma migrate reset  --schema=./prisma/schemas/daphnis/schema.prisma",
